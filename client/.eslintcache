\n        <p className=\"fs-5\">{singleBook.authors}</p>\n        <p className=\"fs-6 fw-light\">{singleBook.publisher}  {singleBook.publishedDate}</p>\n      <p className=\"fs-6\">{singleBook.description}</p>\n    </div>\n</div>\n</div>\n  })}\n</React.Fragment>\n  )\n}","/Users/shane/BOOTCAMP/homework/hw21-google-books/version-1/hw21-googlebooks-search/client/src/components/Home.js",["63"],"import React, {useEffect,useContext} from 'react'\nimport Nav  from './Nav'\nimport BookContext from '../context/books-context'\n\n\nexport default function Home() {\n\n\n  const {searchResults, addBooktoFavorites, savedBooks, removeBookFromFavorites, getSavedBooksFromDatabase,setFavoritesFromDatabase }= useContext(BookContext)\n\n  // GET FAVORITES SAVED IN THE DATABASE AND ADD THEM TO THE STATE\n  useEffect(()=>{\n getSavedBooksFromDatabase()\n .then((resultsFromDatabase)=>{\n  setFavoritesFromDatabase(resultsFromDatabase)\n })\n\n  },[])\n\n\n  return (\n<div>\n  <Nav/>\n{searchResults.map((singleBook)=>{\n\nreturn <div className=\"container border-bottom border-secondary \" key={singleBook.etag}>\n          <div className=\"clearfix\">\n\n              <a className=\"text-decoration-none\" href={singleBook.link}>\n              <img src={singleBook.image} className=\"col-md-6 float-md-start mb-3 ms-md-3 img-effect\" alt=\"...\"/>\n             \n              </a>\n\n              <div className=\"ml-3\">\n                  <p className=\"fs-2 fw-bold\"> \n                  {savedBooks.map(book => book._id).includes(singleBook._id)   ?  <img  className=\"save-icon \"src='./imgs/stared.svg' onClick={()=>{removeBookFromFavorites(singleBook)}} alt=\"Favorite\"/>  :  <img  className=\"save-icon \"src='./imgs/star.svg' onClick={()=>{addBooktoFavorites(singleBook)}} alt=\"Could be a favorite\"/>\n                  \n                  }\n                  {singleBook.title}</p>\n                  <p className=\"fs-5\">{singleBook.authors}</p>\n                  <p className=\"fs-6 fw-light\">{singleBook.publisher}  {singleBook.publishedDate}</p>\n                <p className=\"fs-6\">{singleBook.description}</p>\n              </div>\n          </div>\n</div>\n\n})}\n</div>\n  )\n \n}\n\n// title: { type: String },\n// authors: { type: Array },\n// description: { type: String },\n// image: { type: String },\n// link: { type: String },\n// publisher: { type: String },\n// publishedDate: { type: String }","/Users/shane/BOOTCAMP/homework/hw21-google-books/version-1/hw21-googlebooks-search/client/src/components/Nav.js",["64","65"],"import React, {useRef, useContext} from 'react'\nimport api from '../utils/api'\nimport BookContext from '../context/books-context'\nimport { useHistory } from \"react-router-dom\";\n\nexport default function Nav() {\n  \nconst history = useHistory()  \nconst inputRef = useRef();\nconst {returnedBooks,createBookObject}= useContext(BookContext)\n  \nconst getSearchResults = async(searchStr)=>{\n  const results = await api.getBook(searchStr)\n  return results\n}\n\n\n\nconst handleSubmit = (e)=>{\n  e.preventDefault()\n  let booksObjectFormated = []\n  getSearchResults(inputRef.current.value).then((results)=>{\n    results.data.items.map((result)=>{\n      console.log(\"handling the submit request\", result)\n        const book = createBookObject(result)\n        booksObjectFormated.push(book)\n    })\n    returnedBooks(booksObjectFormated)\n    inputRef.current.value = ''\n   history.push('/')\n  })\n}\n\n{/* <img className=\"img-thumbnail\" src='./imgs/books-75x75.png' style={{maxWidth: \"75px\"}}/> */}\n\n  return (\n   \n    <React.Fragment>\n<nav className=\"navbar navbar-expand-lg navbar-light bg-light\">\n<div className=\"container-fluid\">\n\n    <a className=\"navbar-brand\" href=\"/\">\n    <img className=\"img-thumbnail\" src='./imgs/books-75x75.png' alt='book-icon'/>\n    </a> \n    <a className=\"navbar-brand\" aria-current=\"page\" href=\"/\">BOOK SEARCH</a>\n    <button className=\"navbar-toggler\" type=\"button\" data-bs-toggle=\"collapse\" data-bs-target=\"#navbarSupportedContent\" \n    aria-controls=\"navbarSupportedContent\" aria-expanded=\"false\" aria-label=\"Toggle navigation\">\n      <span className=\"navbar-toggler-icon\"></span>\n    </button>\n    \n    <div className=\"collapse navbar-collapse\" id=\"navbarSupportedContent\">\n  \n    <ul className=\"navbar-nav me-auto mb-2 mb-lg-0\">\n    \n        <li className=\"nav-item\">\n            <a className=\"nav-link active navbar-text\" aria-current=\"page\" href=\"/\">Home</a>\n        </li>\n        <li className=\"nav-item\">\n            <a className=\"nav-link active navbar-text\" aria-current=\"page\" href=\"/saved\">Favorites</a>\n        </li>\n    </ul>\n    </div>\n    </div>\n    <form className=\"d-flex\" onSubmit={handleSubmit}>\n        <input className=\"form-control me-2\"  ref={inputRef}  type=\"search\" placeholder=\"Search\" aria-label=\"Search\"/>\n        <button className=\"btn btn-outline-secondary\" type=\"submit\">Search</button>\n      </form>\n</nav>\n    </React.Fragment>\n  )\n}","/Users/shane/BOOTCAMP/homework/hw21-google-books/version-1/hw21-googlebooks-search/client/src/utils/api.js",["66"],"import axios from \"axios\";\n\nexport default {\n  getBook: function (query) {\n    return axios.get(`https://www.googleapis.com/books/v1/volumes?q=${query}`);\n  },\n  // Deletes the book with the given id\n  deleteBook: function (id) {\n    return axios.delete(\"/api/books/\" + id).then(result => result.data);\n  },\n  // Saves a book to the database\n  saveBook: function (bookData) {\n    return axios.post(\"/api/books\", bookData).then(result => result.data);\n  },\n  // Get the saved a books from the database\n  savedBooks: function () {\n    return axios.get(\"/api/books\").then(result => result.data);\n  }\n};","/Users/shane/BOOTCAMP/homework/hw21-google-books/version-1/hw21-googlebooks-search/client/src/context/BookState.js",[],"/Users/shane/BOOTCAMP/homework/hw21-google-books/